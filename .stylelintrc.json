{
	"extends": ["stylelint-config-standard", "stylelint-config-prettier"],
	"plugins": ["stylelint-order"],
	"rules": {
		"at-rule-no-unknown": [
			true,
			{
				"ignoreAtRules": [
					"tailwind",
					"apply",
					"layer",
					"variants",
					"responsive",
					"screen",
					"custom-variant",
					"theme"
				]
			}
		]
	}
}
